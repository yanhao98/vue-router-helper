{
  "name": "@yanhao98/vue-router-helper",
  "version": "0.0.0",
  "type": "module",
  "description": "",
  "keywords": [
    "vue-router"
  ],
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": {
        "require": "./dist/index.d.cts",
        "import": "./dist/index.d.ts"
      },
      "require": "./dist/index.cjs",
      "import": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "pnpm run build:core",
    "build:core": "tsup",
    "dev": "pnpm run test --ui",
    "lint": "prettier -c '{src,playground}/**/*.{ts,vue}'",
    "play": "npm -C playground run dev",
    "play:build": "npm -C playground run build",
    "release": "node scripts/release.mjs",
    "test": "vitest --typecheck"
  },
  "gitHooks": {
    "pre-commit": "lint-staged",
    "commit-msg": "node scripts/verifyCommit.mjs"
  },
  "lint-staged": {
    "*.{js,mjs,json,cjs,md}": [
      "prettier --write"
    ],
    "*.ts?(x)": [
      "prettier --parser=typescript --write"
    ]
  },
  "dependencies": {
    "@babel/types": "^7.25.2",
    "@rollup/pluginutils": "^5.1.0",
    "@vue-macros/common": "^1.12.2",
    "ast-walker-scope": "^0.6.1",
    "chokidar": "^3.6.0",
    "fast-glob": "^3.3.2",
    "json5": "^2.2.3",
    "local-pkg": "^0.5.0",
    "mlly": "^1.7.1",
    "nprogress": "^0.2.0",
    "pathe": "^1.1.2",
    "scule": "^1.3.0",
    "yaml": "^2.5.0"
  },
  "peerDependencies": {
    "vue-router": "^4.4.0"
  },
  "peerDependenciesMeta": {
    "vue-router": {
      "optional": true
    }
  },
  "devDependencies": {
    "@types/node": "^20.14.15",
    "@types/nprogress": "^0.2.3",
    "chalk": "^5.3.0",
    "enquirer": "^2.4.1",
    "execa": "^9.3.0",
    "lint-staged": "^15.2.8",
    "minimist": "^1.2.8",
    "nodemon": "^3.1.4",
    "p-series": "^3.0.0",
    "pinia": "^2.2.1",
    "prettier": "^3.3.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.20.0",
    "semver": "^7.6.3",
    "tsup": "^8.2.4",
    "typescript": "^5.5.4",
    "unplugin-auto-import": "^0.18.2",
    "vite": "^5.4.0",
    "vite-plugin-vue-devtools": "^7.3.7",
    "vitepress": "1.3.1",
    "vitest": "^2.0.5",
    "vue": "^3.4.37",
    "vue-router": "^4.4.3",
    "vue-router-mock": "^1.1.0",
    "vue-tsc": "^2.0.29",
    "yorkie": "^2.0.0"
  }
}
